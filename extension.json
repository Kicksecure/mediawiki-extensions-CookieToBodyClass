{
	"name": "CookieToBodyClass",
	"author": [
		"Hans (Kicksecure Team)"
	],
	"url": "https://www.mediawiki.org",
	"description": "Output cookies with specific prefix to body class attribute",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\CookieToBodyClass\\": "includes/"
	},
	"HookHandlers": {
		"mainHookHandler": {
			"class": "MediaWiki\\Extension\\CookieToBodyClass\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "mainHookHandler"
	},
	"manifest_version": 2
}
